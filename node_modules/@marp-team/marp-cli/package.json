{
  "name": "@marp-team/marp-cli",
  "version": "1.7.2",
  "description": "A CLI interface for Marp and Marpit based converters",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "cli",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marp-cli"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "lib/index.js",
  "types": "types/src/index.d.ts",
  "files": [
    "marp-cli.js",
    "lib/",
    "types/"
  ],
  "bin": {
    "marp": "marp-cli.js"
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "scripts": {
    "build": "yarn -s clean && rollup -c",
    "build:standalone": "run-s build standalone",
    "check:audit": "yarn audit",
    "check:format": "yarn -s format -c",
    "check:ts": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier \"**/*.{css,js,jsx,json,md,scss,ts,tsx,yaml,yml}\"",
    "format:write": "yarn -s format --write",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --cache .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --parallel build types",
    "preversion": "run-p check:* lint:* test:coverage",
    "standalone": "rimraf bin && pkg --out-path ./bin .",
    "standalone:pack": "node ./scripts/pack.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "types": "rimraf types && tsc --declaration --emitDeclarationOnly --outDir types",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "rollup -w -c"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@rollup/plugin-url": "^6.1.0",
    "@tsconfig/node12": "^1.0.9",
    "@types/cheerio": "^0.22.31",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "~16.11.25",
    "@types/pug": "^2.0.6",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.3",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "bespoke": "bespokejs/bespoke",
    "builtin-modules": "^3.2.0",
    "chalk": "^5.0.1",
    "cheerio": "^1.0.0-rc.10",
    "chrome-launcher": "^0.15.0",
    "cssnano": "^5.1.7",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "express": "^4.17.3",
    "get-stdin": "^9.0.0",
    "globby": "^13.1.1",
    "image-size": "^1.0.1",
    "is-docker": "^3.0.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.1.0",
    "nanoid": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "os-locale": "^6.0.2",
    "pdf-lib": "^1.17.1",
    "pkg": "^5.6.0",
    "pkg-up": "^4.0.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.4.6",
    "postcss-url": "^10.1.3",
    "pptxgenjs": "^3.10.0",
    "prettier": "^2.6.2",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-license": "^2.6.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.50.0",
    "strip-ansi": "^7.0.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "supertest": "^6.2.2",
    "tar-stream": "^2.2.0",
    "ts-jest": "^27.1.4",
    "ts-key-enum": "^3.0.11",
    "tslib": "^2.3.1",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.6.3",
    "vhtml": "^2.2.0",
    "wrap-ansi": "^8.0.1",
    "yauzl": "^2.10.0",
    "zip-stream": "^4.1.0"
  },
  "dependencies": {
    "@marp-team/marp-core": "^2.4.2",
    "@marp-team/marpit": "^2.2.4",
    "chokidar": "^3.5.3",
    "cosmiconfig": "^7.0.1",
    "import-from": "^4.0.0",
    "is-wsl": "^2.2.0",
    "puppeteer-core": "13.6.0",
    "serve-index": "^1.9.1",
    "tmp": "^0.2.1",
    "v8-compile-cache": "^2.3.0",
    "ws": "^8.5.0",
    "yargs": "^17.4.1"
  },
  "resolutions": {
    "pug": "^3.0.2",
    "pug-runtime": "^3.0.1",
    "rollup-plugin-license/moment": "^2.29.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
